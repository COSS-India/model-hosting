name: "speaker_diarization"
backend: "python"
max_batch_size: 16

input [
  {
    name: "AUDIO_DATA"
    data_type: TYPE_STRING
    dims: [ 1 ]
  },
  {
    name: "NUM_SPEAKERS"
    data_type: TYPE_STRING
    dims: [ 1 ]
    optional: true
  }
]

output [
  {
    name: "DIARIZATION_RESULT"
    data_type: TYPE_STRING
    dims: [ 1 ]
  }
]

instance_group [
  {
    count: 1
    kind: KIND_GPU
  }
]

dynamic_batching {
  preferred_batch_size: [ 1, 2, 4, 8, 16 ]
  max_queue_delay_microseconds: 100000
}

